{
  "name": "storage-dao",
  "version": "0.0.8",
  "description": "a data access object that wraps the storage api",
  "main": "dist/webStorage.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "webpack --mode production",
    "test": "jest --coverage",
    "server": "styleguidist server",
    "web-build": "styleguidist build"
  },
  "jest": {
    "testMatch": [
      "<rootDir>/test/?(*.)(spec|test).{js,jsx,ts,tsx}"
    ],
    "transform": {
      "^.+\\.[t|j]s?$": "ts-jest"
    },
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/(?!(lodash-es|other-es-lib))"
    ],
    "testEnvironment": "jsdom",
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Grewer/storage-DAO.git"
  },
  "keywords": [
    "web",
    "storage",
    "localStorage",
    "sessionStorage",
    "dao"
  ],
  "author": "grewer",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Grewer/storage-DAO/issues"
  },
  "homepage": "https://github.com/Grewer/storage-DAO#readme",
  "devDependencies": {
    "@types/jest": "^24.0.24",
    "babel-jest": "^24.9.0",
    "jest": "^24.9.0",
    "react": "^16.12.0",
    "react-docgen-typescript": "^1.16.2",
    "react-dom": "^16.12.0",
    "react-styleguidist": "^10.4.1",
    "ts-jest": "^24.3.0",
    "ts-loader": "^6.2.1",
    "typescript": "^3.6.4",
    "webpack": "^4.41.2",
    "webpack-blocks": "^2.0.1",
    "webpack-cli": "^3.3.10"
  },
  "dependencies": {}
}
